<section class="text-dark has-divider">
  <div class="container">
    <div class="row justify-content-between">
      <div class="col-md-12 text-center">
        <h4 class="text-uppercase mb-4 bizday_sponsors_by_text">Sponsors</h4>
        <ul class="d-flex flex-wrap list-unstyled justify-content-start align-items-center">
            {{#each sponsors}}
            <li class="mt-5 mr-4 mb-5 ml-4 col-lg-3 col-md-4" data-aos="fade-up" data-aos-delay="100">
                <a href="{{url}}">
                    <img class="bg-white icon icon-logo bizday_sponsors_icon {{heightAdjust}}" src="{{image}}" alt="{{name}} logo" />
                </a>
            </li>
            {{/each}}
        </ul>
      </div>
    </div>
  </div>
</section>