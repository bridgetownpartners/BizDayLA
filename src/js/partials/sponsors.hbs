<section class="text-dark has-divider">
  <div class="container">
    <div class="row justify-content-between">
      <div class="col-md-12 text-center">
        <h4 class="text-uppercase mb-4 bizday_sponsors_by_text">Sponsors</h4>
        <ul class="d-flex flex-wrap list-unstyled justify-content-between">
            {{#each sponsors}}
            <li class="ml-4 mr-4 mb-5 col-lg-2 col-md-4" data-aos="fade-up" data-aos-delay="100">
                <a href="{{url}}">
                    <img class="bg-white icon icon-logo bizday_sponsors_icon" src="{{image}}" alt="{{name}}" />
                </a>
            </li>
            {{/each}}
        </ul>
      </div>
    </div>
  </div>
  <div class="divider">
    <img class="bg-white" src="assets/img/dividers/divider-3.svg" alt="divider graphic" data-inject-svg />
  </div>
</section>